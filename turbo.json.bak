{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "SUPABASE_SERVICE_ROLE_KEY", "DEMO_PASSWORD"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [
    "SUPABASE_SERVICE_ROLE_KEY",
    "DEMO_PASSWORD"
  ],
  "globalDependencies": [
    "SUPABASE_SERVICE_ROLE_KEY",
    "DEMO_PASSWORD"
  ]
}
